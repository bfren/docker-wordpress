#!/command/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If WP_SRC doesn't exist, the source files have not been downloaded correctly.
#======================================================================================================================

[[ ! -d ${WP_SRC} ]] && bf-error "WordPress source files cannot be found."


#======================================================================================================================
# Run setup config executable.
#======================================================================================================================

${WP_LIB}/setup-config
